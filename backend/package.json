    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "per-env": "^1.0.2",
    "pg": "^8.4.1",
    "sequelize": "^5.0.0",
    "sequelize-cli": "^5.0.0",
    "uuid": "^8.0.0"
  },
